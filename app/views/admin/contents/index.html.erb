<%
    content_for :title, _('Contents | %{client}') % { client: current_client.name }
    content_for :description, _('Contents of %{client}') % { client: current_client.name }
%>

<article class="card">
  <header class="card-header">
    <h1 class="card-header-title">
      <i class="fa-solid fa-folder"></i>
      &nbsp;
      <%= _('Contents') %>
    </h1>
  </header>
  <div class="card-content">
    <div class="smart-table" id="contents-smart-table" data-use-history="true">
      <%= render partial: 'admin/contents/contents_table' %>
    </div>
  </div>
</article>